<script type="text/javascript"><!--
function show_list(list, user_key){
    var self = [], loop;
    var remove = (list.owner.key == user_key);
    self.push("<li>\n");
    self.push("    <span class=\"image\">\n");
    var title = _('Cover of: %(listname)s', listname=list.name);
    self.push("      <a href=\""); self.push(websafe(list.key)); self.push("\"><img src=\""); self.push(websafe(list.cover_url)); self.push("\" alt=\""); self.push(websafe(title)); self.push("\" title=\""); self.push(websafe(title)); self.push("\"/></a>\n");
    self.push("    </span>\n");
    self.push("    <span class=\"data\">\n");
    self.push("        <span class=\"label\">\n");
    self.push("            <a href=\""); self.push(websafe(list.key)); self.push("\" title=\""); self.push(websafe(_('See this list'))); self.push("\">"); self.push(websafe(list.name)); self.push("</a>\n");
    if (remove) {
        self.push("<a href=\""); self.push(websafe(list.key)); self.push("\" class=\"remove-from-list red smaller arial plain\" data-list-key=\""); self.push(websafe(list.key)); self.push("\" title=\""); self.push(websafe(_('Remove from your list?'))); self.push("\">[X]</a>\n");
    }
    self.push("        </span>\n");
    if (remove) {
        self.push("<span class=\"owner\">from <a href=\""); self.push(websafe(list.owner.key)); self.push("\">"); self.push(websafe(_('You'))); self.push("</a></span>\n");
    }
    else  {
        self.push("<span class=\"owner\">from <a href=\""); self.push(websafe(list.owner.key)); self.push("\">"); self.push(websafe(list.owner.displayname)); self.push("</a></span>\n");
    }
    self.push("    </span>\n");
    self.push("</li>\n");
    self.push("\n");
    return self.join("");
}
//--></script>
<script type="text/javascript"><!--
function render_my_lists(lists, property='active'){
    var self = [], loop;
    foreach(lists, loop, function(loop, list) {
        if (list[property]) {
            self.push("<p class=\"list\">\n");
            self.push("    <a href=\""); self.push(websafe(list.key)); self.push("\" class=\"add-to-list\" data-list-key=\""); self.push(websafe(list.key)); self.push("\">"); self.push(websafe(list.name)); self.push("</a>\n");
            self.push("</p>\n");
            self.push("\n");
        }
    });
    return self.join("");
}
//--></script>
